
<div class="container">
    <div class="row">
        <div class="col s12">
            <h1 class="page-title">カテゴリー一覧</h1>
        </div>
    </div>
    <div class="row">
        <div class="col s12">
            <%= link_to "新規作成",new_category_path,:class=>"waves-effect waves-light btn right" %>
        </div>
    </div>
    <div class="row">
        <ul>
           
        <% @categories.each do |category| %>
            <li>
                <%= link_to category.name,category_path(category) %>
            </li>                
        <% end %>
        </ul>
        
        
        
